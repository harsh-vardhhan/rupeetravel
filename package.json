    "dotenv-cli": "^8.0.0",
    "drizzle-orm": "^0.44.2",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.515.0",
    "next": "^15.1.3",
    "next-sitemap": "4.0.9",
    "react": "^18.2.0",
    "react-dom": "^18",
    "react-intersection-observer": "^9.10.3",
    "react-tweet": "^3.2.0",
    "recharts": "^2.15.3",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
